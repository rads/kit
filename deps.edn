{:deps {io.github.kit-clj/deps-template {:local/root "libs/deps-template"}}
 :aliases {:new {:extra-deps {com.github.seancorfield/clj-new {:mvn/version "1.2.381"}}
                 :exec-fn    clj-new/create
                 :exec-args  {:template kit-clj}}
           :deps-new {:extra-deps {io.github.seancorfield/deps-new {:git/tag "v0.4.13" :git/sha "879c4eb"}}
                      :exec-fn    org.corfield.new/create}
           :build {:deps {io.github.clojure/tools.build {:git/sha "7d40500"
                                                         :git/tag "v0.8.1" :git/url "https://github.com/clojure/tools.build.git"}
                          slipset/deps-deploy {:mvn/version "0.2.0"}
                          weavejester/dependency {:mvn/version "0.2.1"}}
                   :ns-default build}

           :test ;; added by neil
           {:extra-paths ["test"]
            :extra-deps {io.github.cognitect-labs/test-runner
                          {:git/tag "v0.5.0" :git/sha "b3fd0d2"}}
            :main-opts ["-m" "cognitect.test-runner"]
            :exec-fn cognitect.test-runner.api/test}}}
